<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/widget200"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:orientation="vertical"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TextView
android:id="@+id/wlAlphabeticalStatsHeading"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:padding="8px"
android:layout_marginTop="5px"
android:layout_marginBottom="3px"
android:textSize="20sp"
android:textStyle="bold"
android:text="@string/wlByAlpStatsHeading"
android:gravity="center_vertical|center_horizontal"
>
</TextView>
<TableLayout
android:id="@+id/widget201"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="vertical"
>
<TableRow
android:id="@+id/widget202"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:padding="5px"
android:orientation="horizontal"
>
<TextView
android:id="@+id/wl_viewed_words"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:padding="5px"
android:text="Viewed Words : -1"
android:textSize="17sp"
>
</TextView>
</TableRow>
<TableRow
android:id="@+id/widget203"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:padding="5px"
android:orientation="horizontal"
>
<TextView
android:id="@+id/wl_total_words"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:padding="5px"
android:text="Total Words : -1"
android:textSize="17sp"
>
</TextView>
</TableRow>
<TableRow
android:id="@+id/widget204"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:padding="5px"
android:orientation="horizontal"
>
<TextView
android:id="@+id/wl_current_letter"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:padding="5px"
android:text="Current Letter : -1"
android:textSize="17sp"
>
</TextView>
</TableRow>
<TableRow
android:id="@+id/widget205"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:padding="5px"
android:orientation="horizontal"
>
<TextView
android:id="@+id/current_mode"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:padding="5px"
android:text="optionFour"
android:textSize="17sp"
>
</TextView>
</TableRow>
<TableRow
android:id="@+id/widget206"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:padding="5px"
android:orientation="horizontal"
>
<TextView
android:id="@+id/session_marked_words"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:padding="5px"
android:text="optionFive"
android:textSize="17sp"
>
</TextView>
</TableRow>
</TableLayout>
<TableLayout
android:id="@+id/widget207"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="vertical"
>
<TableRow
android:id="@+id/widget208"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:padding="10px"
android:orientation="horizontal"
>
</TableRow>
<TableRow
android:id="@+id/widget208"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:padding="10px"
android:orientation="horizontal"
>
<Button
android:id="@+id/wlByAlpExitButton"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:padding="15px"
android:text="Exit"
>
</Button>
<Button
android:id="@+id/wlByAlpContinueButton"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:padding="15px"
android:text="Continue"
>
</Button>
</TableRow>
</TableLayout>
</LinearLayout>